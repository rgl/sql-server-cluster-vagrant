<?xml version="1.0" encoding="UTF-8"?>
<drivers>
	<provider id="sqlserver">
		<driver id="microsoft" category="MS SQL Server" categories="sql" name="SQL Server" class="com.microsoft.sqlserver.jdbc.SQLServerDriver" url="jdbc:sqlserver://{host}[:{port}][;databaseName={database}]" port="1433" defaultDatabase="master" description="Microsoft JDBC Driver for SQL Server (MSSQL)" custom="false">
			<library type="license" path="https://raw.githubusercontent.com/microsoft/sql-server-samples/master/license.txt" custom="false" disabled="true"/>
			<library type="jar" path="maven:/com.microsoft.sqlserver:mssql-jdbc:RELEASE" custom="false" disabled="true" version="9.2.0.jre8"/>
			<library type="lib" path="maven:/com.microsoft.sqlserver:mssql-jdbc_auth:RELEASE" custom="false" disabled="true" version="9.2.0.x64"/>
			<library type="jar" path="@@SQLJDBC_JAR_PATH@@" custom="true"/>
			<library type="lib" path="@@SQLJDBC_DLL_PATH@@" custom="true"/>
		</driver>
	</provider>
</drivers>